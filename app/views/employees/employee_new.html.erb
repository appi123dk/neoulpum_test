    <div class="middle-box text-center loginscreen  animated fadeInDown">
        <div>
            <div style="padding-top: 70px">

                <h3 class="logo-name">Neulpum</h1>

            </div>
            <h3 style="margin-top: 50px;">지기 등록</h3>
            <p>
                신입지기를 등록해주세요
            </p>
            <form class="m-t form-horizontal" role="form" action="/employees/employee_create" enctype="multipart/form-data" method="POST">
                <div class="form-group">
                    <label class="col-md-3 control-label">학번 : </label>
                    <div class="col-md-9">
                        <input type="text" class="form-control" placeholder="ex : 2015434532" name="student_number" required="">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-3 control-label">이름 : </label>
                    <div class="col-md-9">
                        <input type="text" class="form-control" placeholder="이름을 입력해주세요" name="employee_name" required="">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-3 control-label">전화번호 : </label>
                    <div class="col-md-9">
                        <input type="tel" class="form-control" placeholder="01066669999 (-없이 입력)" name="employee_phone" required="">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-3 control-label">이메일 : </label>
                    <div class="col-md-9">
                        <input type="email" class="form-control" placeholder="neulpum@gmail.com" name="employee_email" required="">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-3 control-label">생년월일 : </label>
                    <div class="col-md-9">
                        <input type="date" class="form-control" name="employee_birth" required="">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-3 control-label">기수 : </label>
                    <div class="col-md-9">
                        <input type="number" class="form-control" placeholder="ex : 11기 -> 11" name="cardinal_number" required="">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-3 control-label">프로필이미지 : </label>
                    <div class="col-md-9">
                        <input type="file" class="form-control" name="employee_image">
                    </div>
                    <small>이미지크기 120 x 120px 준수!!</small>
                </div>
                <button type="submit" class="btn btn-primary block full-width m-b">등록</button>
            </form>
            <!-- <p class="m-t"> <small>Inspinia we app framework base on Bootstrap 3 &copy; 2015</small> </p> -->
        </div>
    </div>